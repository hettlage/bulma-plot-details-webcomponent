{
  "name": "bulma-plot-webcomponent",
  "version": "0.0.1",
  "description": "Web components for displaying a plot",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@stencil/core": "^0.11.4",
    "@types/jest": "^23.3.1",
    "jest": "^23.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saltastro/bulma-plot-details-webcomponent.git"
  },
  "author": "SALT Astronomy Operations",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/saltastro/bulma-plot-details-webcomponent"
  },
  "homepage": "https://github.com/saltastro/bulma-plot-details-webcomponent",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
